         <image>
            <Image url="@homepic2.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="53.0" layoutY="243.0" prefHeight="227.0" prefWidth="330.0" style="-fx-background-color: #e3edee; -fx-background-radius: 13;">
         <children>
            <TextField fx:id="txtid" layoutX="152.0" layoutY="33.0" prefHeight="43.0" prefWidth="162.0" promptText="User name" />
            <TextField fx:id="txtemail" layoutX="153.0" layoutY="81.0" prefHeight="43.0" prefWidth="162.0" promptText="User Email" />
            <Button fx:id="btnfindid" layoutX="153.0" layoutY="140.0" mnemonicParsing="false" onMouseClicked="#findid" prefHeight="49.0" prefWidth="162.0" style="-fx-background-color: #00ff0000; -fx-border-color: #152754; -fx-border-radius: 10;" text="아이디찾기" />
